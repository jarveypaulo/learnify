{
  "name": "learnify-server",
  "version": "0.1.0",
  "description": "Learnify's server npm",
  "main": "server.js",
  "private": true,
  "scripts": {
    "build": "npx webpack",
    "client": "cd client && npm start",
    "coverage": "nyc npm run test",
    "debug": "node inspect server.js",
    "deploy": "gcloud app deploy",
    "dev": "concurrently \"cd src/ && nodemon server.js\" \"cd ../client && npm start\"",
    "start": "concurrently \"cd src/ && node server.js\" \"cd ../client && npm start\"",
    "server": "nodemon server.js",
    "test": "NODE_ENV=test mocha --timeout 5000"
  },
